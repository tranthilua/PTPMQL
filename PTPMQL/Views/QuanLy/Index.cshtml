@model IEnumerable<PTPMQL.Models.QuanLy>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("thêm thông tin quản lý", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenQuanLy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaChi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaQuanLy)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MaQuanLy)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TenQuanLy)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DiaChi)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.MaQuanLy }) |
        @Html.ActionLink("Details", "Details", new { id = item.MaQuanLy }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.MaQuanLy })
    </td>
</tr>
}

</table>
